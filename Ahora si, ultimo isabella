    "# Mapeo del nivel educativo a etiquetas descriptivas\n",
    "mapa_nivel_ed = {\n",
    "    1: 'Primaria Incompleta', 2: 'Primaria Completa',\n",
    "    3: 'Secundaria Incompleta', 4: 'Secundaria Completa',\n",
    "    5: 'Superior Univ. Incompleta', 6: 'Superior Univ. Completa',\n",
    "    7: 'Sin instrucción'\n",
