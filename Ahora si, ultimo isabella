    "props_2005 = props_2005[orden_educativo]\n",
    "props_2025 = props_2025[orden_educativo]\n",
    "\n",
    "# ahora sí, el gráfico en sí\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(18, 8), sharey=True)\n",
